{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": false,
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2017"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "types": [
      "lazy-linq"
    ]
  },
  "include": [
    "./src/**/*"
  ]
}